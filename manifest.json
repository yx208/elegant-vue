{
    "name": "Elegant",
    "description": "An Chrome's new tab extension! To replace Chrome's new tab page of default.",
    "version": "0.0.2",
    "manifest_version": 3,
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "bookmarks",
        "favicon"
    ]
}
